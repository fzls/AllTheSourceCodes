package tests;

public class test_textarea { 
 public static void main (String args[]){
  JFrame f=new JFrame("test");
  f.setBounds(300, 200, 300, 200);
  f.setVisible(true);
  JTextArea t=new JTextArea();
  String str="";
  for(int i=1;i<=10;i++){
   str+="abcdefgh";
   str+='\n';
  }
  t.append(str);
  f.add(t);
 }
}
