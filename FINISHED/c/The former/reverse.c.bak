#include <stdio.h>
int main(void)
{
    long in, res;
    int ri, repeat;
    long reverse(long number); 

    scanf("%d", &repeat);
    for(ri = 1; ri <= repeat; ri++){
        scanf("%ld", &in);
        res=reverse(in);
        printf("%ld\n", res);
    }
}

    long reverse(long number){
    	do{
    		printf("%d",number%10);
    		number/=10;
    		}while(number>0);
    	}
